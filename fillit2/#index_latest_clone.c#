#include "fillit.h"

int		index_latest_clone(int **tet_array, int *tetrimino, int i)
{
	if (i == 0)
        return (-1);
    i--;
	while (i >= 0)
    {
        if (ft_memcmp(tetrimino, tet_array[i], 4) == 0)
		{
			ft_putnbr(tetrimino[2]);
			ft_putnbr(tet_array[i][2]);
			ft_putendl("");
            return (i);
		}
        i--;
    }
    return (-1);
}
